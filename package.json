    "package.json",
    "README.md"
  ],
  "author": "yangxiao <ifyx@qq.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^13.9.1",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "filemanager-webpack-plugin": "^3.0.0",
    "live-server": "^1.2.1",
    "live-server-https": "0.0.2",
    "pre-commit": "^1.2.2",
    "raw-loader": "^4.0.2",
    "ts-loader": "^8.0.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.2.0"
